<% if current_user.strata_employee? %>
<tr class="span-24">
	<td class="span-3"><%= Branch.find(ibtr_resp_stat.respondent_id).name unless ibtr_resp_stat.respondent_id.nil?%><br/></td>
	<td class="span-2"><%= link_to ibtr_resp_stat.assigned_cnt, ibtrs_drillrpt_path(:report => params[:report], :branch_id => ibtr_resp_stat.respondent_id, :state => 'Assigned', :Created=>params[:Created], :start=>params[:start], :end => params[:end] ) %><br/></td>
	<td class="span-2"><%= link_to ibtr_resp_stat.poplaced_cnt, ibtrs_drillrpt_path(:report => params[:report], :branch_id => ibtr_resp_stat.respondent_id, :state => 'POPlaced', :Created=>params[:Created], :start=>params[:start], :end => params[:end] ) %><br/></td>
	<td class="span-2"><%= link_to ibtr_resp_stat.fulfilled_cnt, ibtrs_drillrpt_path(:report => params[:report], :branch_id => ibtr_resp_stat.respondent_id, :state => 'Fulfilled', :Created=>params[:Created], :start=>params[:start], :end => params[:end] ) %><br/></td>
	<td class="span-2"><%= link_to ibtr_resp_stat.dispatched_cnt, ibtrs_drillrpt_path(:report => params[:report], :branch_id => ibtr_resp_stat.respondent_id, :state => 'Dispatched', :Created=>params[:Created], :start=>params[:start], :end => params[:end] ) %><br/></td>
	<td class="span-2"><%= link_to ibtr_resp_stat.received_cnt, ibtrs_drillrpt_path(:report => params[:report], :branch_id => ibtr_resp_stat.respondent_id, :state => 'Received', :Created=>params[:Created], :start=>params[:start], :end => params[:end] ) %><br/></td>
	<td class="span-2"><%= link_to ibtr_resp_stat.declined_cnt, ibtrs_drillrpt_path(:report => params[:report], :branch_id => ibtr_resp_stat.respondent_id, :state => 'Declined', :Created=>params[:Created], :start=>params[:start], :end => params[:end] ) %><br/></td>
  <td class="span-2"><%= link_to ibtr_resp_stat.total_cnt, ibtrs_drillrpt_path(:report => params[:report], :branch_id => ibtr_resp_stat.respondent_id, :state => 'All', :Created=>params[:Created], :start=>params[:start], :end => params[:end] ) %><br/></td>
</tr>
<%else%>
<tr class="span-24">
	<td class="span-3"><%= Branch.find(ibtr_resp_stat.respondent_id).name unless ibtr_resp_stat.respondent_id.nil?%><br/></td>
	<td class="span-2"><%= ibtr_resp_stat.assigned_cnt%><br/></td>
	<td class="span-2"><%= ibtr_resp_stat.poplaced_cnt%><br/></td>
	<td class="span-2"><%= ibtr_resp_stat.fulfilled_cnt%><br/></td>
	<td class="span-2"><%= ibtr_resp_stat.dispatched_cnt%><br/></td>
	<td class="span-2"><%= ibtr_resp_stat.received_cnt%><br/></td>
	<td class="span-2"><%= ibtr_resp_stat.declined_cnt%><br/></td>
  <td class="span-2"><%= ibtr_resp_stat.total_cnt%><br/></td>
</tr>

<%end%>
